<?xml version="1.0" encoding="utf-8"?>

<animated-vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt">

    <aapt:attr
        name="android:drawable">

        <vector
            android:name="av_list_show_hide_24dp"
            android:height="24dp"
            android:width="24dp"
            android:viewportHeight="24"
            android:viewportWidth="24">

            <path
                android:name="bar"
                android:fillColor="?attr/toolbarIconColor"
                android:pathData="M 7 6 L 9 6 L 9 18 L 7 18 L 7 6 Z">
            </path>

            <path
                android:name="outline"
                android:fillColor="?attr/toolbarIconColor"
                android:pathData="M 20 4 L 4 4 C 2.9 4 2 4.9 2 6 L 2 18 C 2 19.1 2.9 20 4 20 L 20 20 C 21.1 20 22 19.1 22 18 L 22 6 C 22 4.9 21.1 4 20 4 Z M 20 18 L 9 18 L 7 18 L 4 18 L 4 6 L 7 6 L 9 6 L 20 6 L 20 18 Z">
            </path>

        </vector>

    </aapt:attr>

    <target
        android:name="bar">

        <aapt:attr
            name="android:animation">

            <objectAnimator
                android:duration="@integer/time_animation"
                android:interpolator="@android:interpolator/fast_out_slow_in"
                android:propertyName="pathData"
                android:valueFrom="M 7 6 L 9 6 L 9 18 L 7 18 L 7 6 Z"
                android:valueTo="M 2 6 L 4 6 L 4 18 L 2 18 L 2 6 Z"
                android:valueType="pathType">
            </objectAnimator>

        </aapt:attr>

    </target>

</animated-vector>
